{
  "name": "learn-webpack-babel6",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "loaders": "NODE_ENV=test node ./loaders-by-extension-test.js | tap-notify | tap-diff",
    "sass-dev": "NODE_ENV=development node ./node_modules/webpack/bin/webpack.js --progress --profile --verbose --colors --display-error-details --config src/sass/webpack-dev.config.js",
    "sass-pro": "NODE_ENV=production node ./node_modules/webpack/bin/webpack.js --progress --profile --verbose --colors --display-error-details --config src/sass/webpack-pro.config.js",
    "es6-dev": "NODE_ENV=development node ./node_modules/webpack/bin/webpack.js --progress --profile --verbose --colors --display-error-details --config src/js/es6/webpack-dev.config.js",
    "es6-pro": "NODE_ENV=production node ./node_modules/webpack/bin/webpack.js --progress --profile --verbose --colors --display-error-details --config src/js/es6/webpack-pro.config.js",
    "es6-test": "NODE_ENV=test node ./node_modules/webpack/bin/webpack.js --progress --profile --verbose --colors --display-error-details --config src/js/es6/webpack-test.config.js && node src/js/es6/build/main.js | tap-notify | tap-diff",
    "decorator-test": "NODE_ENV=test node ./node_modules/webpack/bin/webpack.js --progress --profile --verbose --colors --display-error-details --config src/js/decorator/webpack-test.config.js && node src/js/decorator/build/main.js | tap-notify | tap-diff",

    "css-dev:test": "NODE_ENV=development webpack --config ./src/css/webpack.dev.babel",
    "css-pro:test": "rm -rf ./build/css/* && NODE_ENV=production webpack --config ./src/css/webpack.prod.babel",
    "css-test:test": "NODE_ENV=test webpack --config ./src/css/webpack.test.babel",

    "html-pro:test": "rm -rf ./build/html/* && NODE_ENV=production webpack --config ./src/html/webpack.prod.babel",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "assets-webpack-plugin": "^3.3.0",
    "autoprefixer": "^6.3.1",
    "autoprefixer-loader": "^3.2.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-cli": "6.14.0",
    "babel-core": "6.14.0",
    "babel-eslint": "6.1.2",
    "babel-loader": "6.2.5",
    "babel-plugin-istanbul": "2.0.1",
    "babel-plugin-react-intl": "2.2.0",
    "babel-plugin-react-transform": "2.0.2",
    "babel-plugin-transform-react-constant-elements": "6.9.1",
    "babel-plugin-transform-react-inline-elements": "6.8.0",
    "babel-plugin-transform-react-remove-prop-types": "0.2.9",
    "babel-preset-latest": "6.14.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-0": "6.5.0",

    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "node-sass": "^3.4.2",

    "postcss-cssnext": "2.8.0",
    "postcss-focus": "1.0.0",
    "postcss-loader": "0.13.0",
    "postcss-reporter": "1.4.1",

    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.1",
    "tap-diff": "^0.1.1",
    "tap-notify": "^1.0.0",
    "tape": "^4.4.0",
    "webpack": "^2.1.0-beta.25",
    "extract-text-webpack-plugin": "2.0.0-beta.4"
  },
  "dependencies": {
    "react": "15.3.1",
    "react-dom": "15.3.1"
  },
  "babel": {
    "presets": [
      [
        "latest",
        {
          "es2015": {
            "modules": false
          }
        }
      ],
      "react",
      "stage-0"
    ],
    "env": {
      "production": {
        "only": [
          "app"
        ],
        "plugins": [
          "transform-react-remove-prop-types",
          "transform-react-constant-elements",
          "transform-react-inline-elements"
        ]
      },
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    }
  }
}
